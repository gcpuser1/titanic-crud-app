FROM openjdk:8u191-jre-alpine
WORKDIR /app
COPY target/Titanic-0.0.1.jar .
ENV SPRING_DATASOURCE_URL=jdbc:mysql://localhost:8000/
ENV SPRING_DATASOURCE_PASSWORD=admin12345
ENV SPRING_DATASOURCE_USERNAME=root
ENV SERVER_PORT=9000
ENV SPRING_DATASOURCE_DRIVER_CLASS_NAME=org.postgresql.Driver
EXPOSE $SERVER_PORT
ENTRYPOINT ["java","-jar","Titanic-0.0.1.jar"]
